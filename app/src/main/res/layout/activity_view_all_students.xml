<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewAllStudents">

    <TextView
        android:id="@+id/view_all_act"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/view_all"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/studentsList"
        android:layout_width="346dp"
        android:layout_height="526dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="@+id/view_all_act"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/view_all_act"
        app:layout_constraintTop_toBottomOf="@+id/view_all_act" />
</androidx.constraintlayout.widget.ConstraintLayout>